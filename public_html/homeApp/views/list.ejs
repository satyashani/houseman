<div class="row"><%- result.paging? result.paging : "" %></div>
<div class="row">
    <a href="/list/editFields" class="fancybox btn btn-primary btn-sm pull-right">Select table fields</a>
</div>
<table class="table table-striped">
	<% if(typeof result !== 'undefined') { %>
	<thead>
	<tr>
		<%
		result.labels.forEach(function(c){ %>
		<th><%- c %></th>
		<% }); %>
	</tr>
	</thead>
	<tbody>
	<% result.rows.forEach(function(row){ %>
	<tr>
		<% result.keys.forEach(function(k){ %>
		<td>
                    <% if(row[k] && row[k].url) { %>
                    <a href="<%- row[k].url %>" class='fancybox'><%= row[k].label %></a>
                    <% }else{ %>
                    <%- row[k] %>
                    <% } %>
                </td>
		<%	});	%>
		<% if(row.actions) { %>
		<td>
			<% row.actions.forEach(function(a){ %>
			<a href="<%- a.url %>" class='btn btn-default btn-sm fancybox'><%= a.label %></a>
			<% }); %>
		</td>
		<% } %>
	</tr>
	<% }); %>
	</tbody>
	<% } %>
</table>