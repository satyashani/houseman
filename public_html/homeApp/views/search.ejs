<div class="col-lg-8 col-md-8 col-sm-8 pull-left">
	<form id="searchForm" class="form-inline" role="search" action="<%= action %>" method="post">
		<div class="form-group">
			<input id="search" type="text" class="form-control" placeholder="<%= search_string %>" name="term" autocomplete="off" style="width: 300px;"/>
		</div>
		<button type="submit" class="btn btn-default">Search</button>
	</form>
	<div id="suggest" style="position: absolute; top: 100%;">

	</div>
</div>